<%- include("header") -%>
<body>
    <div class="box" id="heading">
    <h1> <%= listtitle %> </h1>
</div>
<div class="box">
        <% for( var i=0;i<listitem.length;i++){%>
            <div class="item">
                <input type="checkbox">
        <p><%=  listitem[i] %></p>
    </div>
        <% } %>
    <form action="/" method="post">
        <input type="text" name="text" placeholder="New item" autocomplete="off">
        <button type="submit" name="submit">+</button>
    </form>
</div>
</body>
<%- include("footer") -%>
